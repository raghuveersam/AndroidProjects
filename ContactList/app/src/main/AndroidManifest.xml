<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raghuveer.contactlist" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon1_hw"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateContact"
            android:label="@string/title_activity_create_contact" >
        </activity>
        <activity
            android:name=".EditContact"
            android:label="@string/title_activity_edit_contact" >
        </activity>
        <activity
            android:name=".DeleteContact"
            android:label="@string/title_activity_delete_contact" >
        </activity>
        <activity
            android:name=".ViewContact"
            android:label="@string/title_activity_view_contact" >
        </activity>
    </application>
    <uses-permission android:name="com.google.android.apps.photos.permission.GOOGLE_PHOTOS"/>
    <uses-permission android:name="com.google.android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CALL_PHONE" />

</manifest>
